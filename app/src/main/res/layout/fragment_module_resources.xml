<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <data>

    </data>

    <androidx.core.widget.NestedScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/md_theme_light_background"
        android:fillViewport="true">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/linearLayout"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <Button
                android:id="@+id/button17"
                android:layout_width="269dp"
                android:layout_height="96dp"
                android:backgroundTint="#95C99B"
                android:fontFamily="@font/lato_bold"
                android:text="Slides"
                android:textColor="@color/md_theme_light_onBackground"
                android:textSize="34sp"
                app:cornerRadius="@dimen/fab_margin"
                app:layout_constraintBottom_toTopOf="@+id/button18"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.481"
                tools:layout_conversion_absoluteHeight="48dp"
                tools:layout_conversion_absoluteWidth="88dp" />

            <Button
                android:id="@+id/button18"
                android:layout_width="269dp"
                android:layout_height="96dp"
                android:layout_marginBottom="172dp"
                android:backgroundTint="#95C99B"
                android:fontFamily="@font/lato_bold"
                android:text="Labs"
                android:textColor="@color/md_theme_light_onBackground"
                android:textSize="34sp"
                app:cornerRadius="@dimen/fab_margin"
                app:layout_constraintBottom_toTopOf="@+id/web_view"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                tools:layout_conversion_absoluteHeight="48dp"
                tools:layout_conversion_absoluteWidth="88dp" />

            <WebView
                android:id="@+id/web_view"
                android:layout_width="15dp"
                android:layout_height="6dp"
                android:layout_marginBottom="100dp"
                android:background="@color/md_theme_light_background"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                tools:layout_conversion_absoluteHeight="746dp"
                tools:layout_conversion_absoluteWidth="411dp" />
        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.core.widget.NestedScrollView>

</layout>